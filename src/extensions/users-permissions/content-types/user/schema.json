{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "badges_given": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::badge.badge",
      "mappedBy": "user_who_gave_the_vote"
    },
    "badges_received": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::badge.badge",
      "mappedBy": "user_who_received_the_vote"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "users_permissions_users"
    },
    "invitations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invitation.invitation",
      "mappedBy": "inviter"
    },
    "nominations_given": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nomination.nomination",
      "mappedBy": "user_who_give_vote"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "users_permissions_users"
    },
    "nominations_received": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nomination.nomination",
      "mappedBy": "user_who_receive_vote"
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "required": false
    },
    "education": {
      "type": "string",
      "required": true
    },
    "job": {
      "type": "string"
    },
    "family": {
      "type": "string"
    },
    "quote": {
      "type": "text"
    },
    "bio": {
      "type": "text"
    }
  }
}
